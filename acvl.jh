entity Secouriste {
    nom String required,
    prenom String required,
    adresse String,
    telephone String, 
    centre TypeCentre,
    etat Etat,
    estMobile Boolean,
    estFixe Boolean,
    estAmbulancier Boolean
}

service * with serviceClass
dto * with mapstruct